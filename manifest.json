{
    "manifest_version": 2,

        "name": "Internet Achievements",
        "description": "",
        "version": "0.1",
        "background": {
        },
        "browser_action": {
            "default_title": "Internet Achievements"
        },
        "permissions": [
            "activeTab",
            "tabs", "http://*/*", "https://*/*"
        ],
        "background": {
            "scripts": ["background.js"],
            "persistent": false
        },
        "content_scripts": [ {
            "matches" : ["http://*/*", "https://*/*"],
            "js": ["content_script.js"]
        } ]
}
